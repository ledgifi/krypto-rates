generator client {
  provider = "prisma-client-js"
  output   = "../../../node_modules/@prisma/client"
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

model Rate {
  id         String   @default(cuid()) @id
  createdAt  DateTime @default(now())
  source     String
  // TODO: JSON type unsupported
  // sourceData Json
  market     String
  timestamp  DateTime
  value      Float
}
